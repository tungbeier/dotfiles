[core]
	editor = vim
	fileMode = false
	excludesfile = /home/tung/.config/git/git-templates/gitignore_global
[init]
	templatedir = /home/tung/.config/git/git-templates
[commit]
	template = /home/tung/.config/git/git-templates/git_commit_msg.txt
[alias]
	i = init
	cl = clone
	st = status -sb --show-stash
	d = diff
	ds = diff --staged
	pl = up
	up = "!git remote update -p; git merge --ff-only @{u}"
	ps = push
	psh = push --set-upstream origin HEAD
	psf = push --force-with-lease
	f = fetch
	l = log
	ll = log -1
	a = add
	c = commit --verbose
	cp = cherry-pick
	br = branch
	co = checkout
	sh = stash
	shl = stash list
	sha = stash apply
	shp = stash pop
	re = rebase
	rei = rebase -i
	unstage = reset HEAD

